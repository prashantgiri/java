    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-DemoApplication<demo>",
            "request": "launch",
            "mainClass": "demo.example.demo.DemoApplication",
            "projectName": "demo"
        },
        {
            "type": "java",
            "name": "Spring Boot-DemoApplication<springBootDemo>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "mainClass": "com.technoviral.prashant.DemoApplication",
            "projectName": "springBootDemo",
            "args": ""
        }
    ]
}